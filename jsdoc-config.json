{
  "source": {
    "include": ["./src/engine"],
    "includePattern": ".+\\.js$",
    "excludePattern": "node_modules/"
  },
  "sourceType": "module",
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "readme": "./README.md",
    "destination": "./docs",
    "recurse": true,
    "verbose": true,
    "template": "./node_modules/docdash",
    "theme_opts": {
      "default_theme": "dark"
    }
  },
  "markdown": {
    "hardwrap": false,
    "idInHeadings": true
  }
}
